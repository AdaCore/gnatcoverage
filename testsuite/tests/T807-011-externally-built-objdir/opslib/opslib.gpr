library project Opslib is

   for Library_Name use "opslib";
   for Library_Kind use "static";

   for Library_Dir use "lib-" & Project'Library_Name;
   for Object_Dir use "obj-" & Project'Library_Name;

   for Source_Dirs use (".");

   type Boolean is ("false", "true");
   for Externally_Built use external ("OPSLIB_EXTERNALLY_BUILT", "false");

end Opslib;
