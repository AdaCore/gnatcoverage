   <set name="$show_decisions_d2" type="Decision"/>
   <set name="$show_decisions_d3" type="Decision"/>
   <set name="nodes" type="Decision_Element"/>
</node>

<node color="Green">
   <type name="Decision_Element"/>
</node>

<node visible="no">
   <type name="B/BDD"/>
